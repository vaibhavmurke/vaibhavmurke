#include <iostream>
using namespace std;

int main()
{
    int n = 3;
    // cin >> n;
    int count = 1;
    // cout << "Enter the value of n:" << endl;
    for (int row = 1; row <= n; row++)
    {
        for (int col = 1; col <= row; col++)
        {
            cout << count << " ";
            count++;
        }
        cout << endl;
    }
    return 0;
};
