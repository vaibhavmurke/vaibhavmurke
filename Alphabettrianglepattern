#include <iostream>
using namespace std;
void Alphabettriangle(int n)
{
    for (int i = 1; i <= n; i++)
    {
        char ch = 'A';
        for (int j = 1; j <= i; j++)
        {
            cout << ch;
            ch++;
        }
        cout << endl;
    }
}
int main()
{
    Alphabettriangle(5);

    return 0;
}
